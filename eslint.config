{
  parserOptions: {
    ecmaFeatures: {
      jsx: true,
    },
    ecmaVersion: 2018,
    sourceType: 'module',
  },
  parser: babel-eslint,
  plugins: ['react'],
  rules: {
    'react/jsx-filename-extension': 0,
    'no-shadow': 0,
    'import/no-extraneous-dependencies': 0,
    'camelcase': 0,
    'import/no-unresolved": "off"',
  }
}